<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 <mapper namespace="MailMapper">
	<select id="Mail_Info" parameterType="int" resultType="MailVO">
		SELECT * 
		FROM mail
		WHERE port = #{port};
	</select>
</mapper>		